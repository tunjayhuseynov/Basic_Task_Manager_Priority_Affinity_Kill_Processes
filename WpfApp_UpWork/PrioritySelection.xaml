<Window x:Class="WpfApp_UpWork.PrioritySelection"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp_UpWork"
        mc:Ignorable="d"
        Title="PrioritySelectionPage" Height="250" Width="200">
    <Grid>
        <RadioButton Tag="256" Content="Real Time" Name="rt" Checked="OnChecked" HorizontalAlignment="Left" Margin="50,50,0,0" VerticalAlignment="Top"/>
        <RadioButton Tag="128" Content="High" Name="high" Checked="OnChecked" HorizontalAlignment="Left" Margin="50,70,0,0" VerticalAlignment="Top"/>
        <RadioButton Tag="32768" Content="Above Normal" Name="anormal" Checked="OnChecked" HorizontalAlignment="Left" Margin="50,90,0,0" VerticalAlignment="Top"/>
        <RadioButton Tag="32" Content="Normal" Name="normal" Checked="OnChecked" HorizontalAlignment="Left" Margin="50,110,0,0" VerticalAlignment="Top"/>
        <RadioButton Tag="16384" Content="Below Normal" Name="bnormal" Checked="OnChecked" HorizontalAlignment="Left" Margin="50,130,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
